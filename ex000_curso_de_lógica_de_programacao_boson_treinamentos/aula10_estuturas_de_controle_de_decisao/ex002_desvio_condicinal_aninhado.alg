algoritmo "Nome do Programa"

var
a: inteiro
inicio

// desvio condicional aninhado, significa um valor dentro de outro
a <- 3
se a = 4 entao
   escreva("A igual a 4")
senao
  se a = 5 entao
     escreva("A igual a 5")
  senao
     se a > 5 entao
       escreva("A maior que 5")
     senao
       escreva("Valor fora do limite definido")
     fimse
  fimse
fimse
fimalgoritmo
