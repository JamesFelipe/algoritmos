algoritmo "Nome do Programa"

var

idade, valor: inteiro
nome: caractere
salario: real

inicio

// Atribuindo valores
//idade <- 25
//nome <- "James"
//valor <- 4500
//salario <- valor

// O comando escreval vai escrever na tela e pular para a linha debaixo
escreval(idade)
escreval(nome)
escreval(salario)
fimalgoritmo
