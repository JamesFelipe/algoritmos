algoritmo "semnome"
var
op, i: inteiro
inicio
 escreval("Lanche Aleatorio")
 escreval("---------------------")
 escreval("escolha um número: (1 2 3)")
 leia(op)
  escolha op
   caso 1
    escreva("Hotdog")
   caso 2
    escreva("Brigadeiro")
    caso 3
     escreva("Bolo")
    outrocaso
     escreva("Erro")
     fimescolha
fimalgoritmo
