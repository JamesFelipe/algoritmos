algoritmo "FuncaoSe"
var
  i: inteiro
  j: caractere
inicio
      escreva("Vamos comer aleatoriamente")
      escreva("Escolha um número (1 2 3): ")
      leia(i)
       se i = 1 entao
        escreva("Macarão")
        fimse
       se i = 2 entao
        escreva("Meat with rice and beans")
        fimse
         se i = 3 entao
          escreva("Açaí")
          fimse
fimalgoritmo
