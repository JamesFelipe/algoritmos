algoritmo "salario"

var
   nome: caracter
   salario: real
inicio
   escreva("Qual seu nome? ")
   leia(nome)
   escreva("Informe o seu salário: ")
   leia(salario)
   escreval("Nome do Funcionário: ", nome)
   escreval("Salário: ", salario)
   escreva("O funcionário ", nome, " tem um salário de R$", salario, " em Junho.")
fimalgoritmo
