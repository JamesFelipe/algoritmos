algoritmo "tempo_de_servico"

var
dias_trabalhados, horas_trabalhadas, valor_hora: real
inicio
escreva("Digite quantos dias você trabalhou em um mês: ")
leia(dias_trabalhados)
valor_hora <- 25
escreva("Você trabalhou ", dias_trabalhados, " dias e vai ganhar ", (dias_trabalhados * 8) * valor_hora, " R$")

fimalgoritmo

